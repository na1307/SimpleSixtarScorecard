<Window x:Class="SimpleSixtarScorecard.Experimental.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleSixtarScorecard.Experimental"
        xmlns:blazor="http://schemas.microsoft.com/winfx/2006/xaml/presentation/blazor"
        mc:Ignorable="d"
        Title="SimpleSixtarScorecard" Height="800" Width="1280" WindowStartupLocation="CenterScreen">
    <blazor:BlazorWebView x:Name="bw" HostPage="wwwroot\index.html">
        <blazor:BlazorWebView.RootComponents>
            <blazor:RootComponent Selector="#app" ComponentType="{x:Type local:Routes}" />
        </blazor:BlazorWebView.RootComponents>
    </blazor:BlazorWebView>
</Window>
